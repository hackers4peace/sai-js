{
  "@context": [
    "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^8.0.0/components/context.jsonld",
    "https://linkedsoftwaredependencies.org/bundles/npm/asynchronous-handlers/^1.0.0/components/context.jsonld",
    "https://linkedsoftwaredependencies.org/bundles/npm/@elfpavlik/sai-components/^1.0.0/components/context.jsonld"
  ],
  "import": [
    "css:config/app/init/static-root.json",
    "css:config/app/main/default.json",
    "css:config/app/variables/default.json",
    "sai:config/http/handler/default.json",
    "css:config/http/middleware/default.json",
    "css:config/http/notifications/disabled.json",
    "css:config/http/server-factory/https.json",
    "css:config/http/static/default.json",
    "css:config/identity/access/public.json",
    "css:config/identity/email/default.json",
    "css:config/identity/handler/no-pods.json",
    "css:config/identity/oidc/default.json",
    "css:config/identity/ownership/unsafe-no-check.json",
    "css:config/identity/pod/static.json",
    "css:config/ldp/authentication/dpop-bearer.json",
    "css:config/ldp/authorization/allow-all.json",
    "css:config/ldp/handler/disabled.json",
    "css:config/ldp/metadata-parser/default.json",
    "css:config/ldp/metadata-writer/default.json",
    "css:config/ldp/modes/default.json",
    "css:config/storage/backend/file.json",
    "sai:config/storage/key-value/memory.json",
    "sai:config/storage/key-value/postgres.json",
    "css:config/storage/location/root.json",
    "css:config/storage/middleware/default.json",
    "css:config/util/auxiliary/empty.json",
    "css:config/util/identifiers/suffix.json",
    "css:config/util/index/default.json",
    "css:config/util/logging/winston.json",
    "css:config/util/representation-conversion/default.json",
    "css:config/util/resource-locker/memory.json",
    "css:config/util/variables/default.json"
  ],
  "@graph": [
    {
      "comment": [
        "A Solid server that only supports account management and OIDC interaction, without pods.",
        "Note that several of the imports above are irrelevant due to there not being pods."
      ]
    },
    {
      "comment": "Vapid private key.",
      "@id": "urn:sai:default:variable:vapidPublicKey",
      "@type": "Variable"
    },
    {
      "comment": "Vapid private key.",
      "@id": "urn:sai:default:variable:vapidPublicKey",
      "@type": "Variable"
    },
    {
      "comment": "Push sender.",
      "@id": "urn:sai:default:variable:pushSender",
      "@type": "Variable"
    },
    {
      "comment": "Encoded private JWK.",
      "@id": "urn:sai:default:variable:encodedPrivateJwk",
      "@type": "Variable"
    },
    {
      "comment": "PostgreSQL connection string.",
      "@id": "urn:sai:default:variable:postgresConnectionString",
      "@type": "Variable"
    },
    {
      "comment": "Authorization endpoint.",
      "@id": "urn:sai:default:variable:authorizationEndpoint",
      "@type": "Variable"
    },
    {
      "@id": "urn:solid-server-app-setup:default:CliExtractor",
      "@type": "YargsCliExtractor",
      "parameters": [
        {
          "@type": "YargsParameter",
          "name": "vapidPublicKey",
          "options": {
            "demandOption": true,
            "requiresArg": true,
            "type": "string",
            "describe": "VAPID public key (Push Notifications)."
          }
        },
        {
          "@type": "YargsParameter",
          "name": "vapidPrivateKey",
          "options": {
            "demandOption": true,
            "requiresArg": true,
            "type": "string",
            "describe": "VAPID private key (Push Notifications)."
          }
        },
        {
          "@type": "YargsParameter",
          "name": "pushSender",
          "options": {
            "demandOption": true,
            "requiresArg": true,
            "type": "string",
            "describe": "IRI denoting push notifications sender (mailto: or https:)."
          }
        },
        {
          "@type": "YargsParameter",
          "name": "encodedPrivateJwk",
          "options": {
            "demandOption": true,
            "requiresArg": true,
            "type": "string",
            "describe": "Base64url encoded private JWK."
          }
        },
        {
          "@type": "YargsParameter",
          "name": "postgresConnectionString",
          "options": {
            "demandOption": true,
            "requiresArg": true,
            "type": "string",
            "describe": "PostgreSQL connection string."
          }
        },
        {
          "@type": "YargsParameter",
          "name": "authorizationEndpoint",
          "options": {
            "demandOption": true,
            "requiresArg": true,
            "type": "string",
            "describe": "Authorization endpoint."
          }
        }
      ]
    },
    {
      "@id": "urn:solid-server-app-setup:default:ShorthandResolver",
      "@type": "CombinedShorthandResolver",
      "resolvers": [
        {
          "CombinedShorthandResolver:_resolvers_key": "urn:sai:default:variable:vapidPublicKey",
          "CombinedShorthandResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "vapidPublicKey"
          }
        },
        {
          "CombinedShorthandResolver:_resolvers_key": "urn:sai:default:variable:vapidPrivateKey",
          "CombinedShorthandResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "vapidPrivateKey"
          }
        },
        {
          "CombinedShorthandResolver:_resolvers_key": "urn:sai:default:variable:pushSender",
          "CombinedShorthandResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "pushSender"
          }
        },
        {
          "CombinedShorthandResolver:_resolvers_key": "urn:sai:default:variable:encodedPrivateJwk",
          "CombinedShorthandResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "encodedPrivateJwk"
          }
        },
        {
          "CombinedShorthandResolver:_resolvers_key": "urn:sai:default:variable:postgresConnectionString",
          "CombinedShorthandResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "postgresConnectionString"
          }
        },
        {
          "CombinedShorthandResolver:_resolvers_key": "urn:sai:default:variable:authorizationEndpoint",
          "CombinedShorthandResolver:_resolvers_value": {
            "@type": "KeyExtractor",
            "key": "authorizationEndpoint"
          }
        }
      ]
    },
    {
      "@id": "urn:sai:default:UiPushSubscriptionStore",
      "@type": "UiPushSubscriptionStore",
      "storage": { "@id": "urn:solid-server:default:AccountStorage" }
    },
    {
      "@id": "urn:sai:default:ReciprocalWebhookStore",
      "@type": "ReciprocalWebhookStore",
      "storage": { "@id": "urn:solid-server:default:AccountStorage" }
    },
    {
      "@id": "urn:solid-server:default:PrimaryParallelInitializer",
      "@type": "ParallelHandler",
      "handlers": [
        { "@id": "urn:solid-server:default:KeyValueStorage" },
        { "@id": "urn:sai:default:UiPushSubscriptionStore" },
        { "@id": "urn:sai:default:ReciprocalWebhookStore" }
      ]
    },
    {
      "@id": "urn:solid-server:default:EarlyProcessParallelInitializer",
      "@type": "ParallelHandler",
      "handlers": [
        { "@id": "urn:solid-server:default:KeyValueStorage" },
        { "@id": "urn:sai:default:UiPushSubscriptionStore" },
        { "@id": "urn:sai:default:ReciprocalWebhookStore" }
      ]
    },
    {
      "@id": "urn:sai:default:OverrideJwkGenerator",
      "@type": "Override",
      "overrideInstance": {
        "@id": "urn:solid-server:default:JwkGenerator"
      },
      "overrideParameters": {
        "@type": "EnvJwkGenerator",
        "encodedPrivateJwk": { "@id": "urn:sai:default:variable:encodedPrivateJwk" }
      }
    },
    {
      "@id": "urn:sai:default:OverrideWebIdStore",
      "@type": "Override",
      "overrideInstance": {
        "@id": "urn:solid-server:default:WebIdStore"
      },
      "overrideParameters": {
        "@type": "CustomWebIdStore",
        "storage": { "@id": "urn:solid-server:default:AccountStorage" }
      }
    }
  ]
}
