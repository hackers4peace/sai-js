{
  "buildCommand": "vite build && node scripts/vercel.js",

  "rewrites": [
    {
      "source": "/id",
      "destination": "/id.jsonld"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],

  "headers": [
    {
      "source": "/id.jsonld",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/ld+json"
        }
      ]
    }
  ]
}
